<?xml version="1.0"?>
<menus xmlns="http://www.rforge.net/CADStat">
  <menu>
    <name>Edit</name>
    <separator/>
    <menuitem>
      <name>JGR Preferences</name>
      <command>.jnew('org/rosuda/JGR/toolkit/PrefsDialog')</command>
    </menuitem>
    <menuitem>
      <name>CADStat Preferences</name>
      <command>load.class('org/neptuneinc/cadstat/prefs/PreferencesDialog')</command>
    </menuitem>
  </menu>
  <menu>
    <name>Tools</name>
    <separator/>
    <menuitem>
      <name>Package Manager</name>
      <command>package.manager()</command>
    </menuitem>
    <menuitem>
      <name>Package Installer</name>
      <command>installPackages()</command>
    </menuitem>
    <separator/>
    <menuitem>
      <name>Data Merge</name>
      <command>load.class('org/neptuneinc/cadstat/plots/DataMergeDialog');</command>
    </menuitem>
  </menu>
  <menu>
    <name>Workspace</name>
    <separator/>
    <menuitem>
      <name>Clean CADStat File Workspace</name>
      <command>cleanCADStatWorkspace()</command>
    </menuitem>
  </menu>
  <menu>
    <name>Graph</name>
    <menuitem>
      <name>Boxplot</name>
      <command>load.class('org/neptuneinc/cadstat/plots/BoxPlot')</command>
    </menuitem>
    <menuitem>
      <name>Scatterplot</name>
      <command>load.class('org/neptuneinc/cadstat/plots/ScatterPlot')</command>
    </menuitem>
  </menu>
  <menu>
    <name>Analysis Tools</name>
    <menuitem>
      <name>Linear Regression</name>
      <command>load.class('org/neptuneinc/cadstat/plots/LinearRegression')</command>
    </menuitem>
    <menuitem>
      <name>Regression Prediction</name>
      <command>load.class('org/neptuneinc/cadstat/plots/RegressionPrediction')</command>
    </menuitem>
    <menuitem>
      <name>Quantile Regression</name>
      <command>load.class('org/neptuneinc/cadstat/plots/QuantileRegression')</command>
    </menuitem>
    <menuitem>
      <name>Correlation Analysis</name>
      <command>load.class('org/neptuneinc/cadstat/plots/CorrelationAnalysis')</command>
    </menuitem>
    <menuitem>
      <name>Tree Regression</name>
      <command>load.class('org/neptuneinc/cadstat/plots/TreeRegression')</command>
    </menuitem>
    <menuitem>
      <name>Factor Analysis</name>
      <command>load.class('org/neptuneinc/cadstat/plots/FactorAnalysis')</command>
    </menuitem>
    <separator/>
    <menuitem>
      <name>Conditional Probability</name>
      <command>load.class('org/neptuneinc/cadstat/plots/ConditionalProbability')</command>
    </menuitem>
    <menuitem>
      <name>Predicting Env Conditions from Bio Observations</name>
      <command>load.class('org/neptuneinc/cadstat/plots/BiologicalInferences')</command>
    </menuitem>
    <separator/>
  </menu>
  <menu>
    <name>Help</name>
    <menuitem>
      <name>R Help</name>
      <command>browseURL(paste("file:///",file.path(R.home('doc'),'html', 'index.html'), sep = ""))</command>
    </menuitem>
    <separator/>
    <menuitem>
      <name>Loading and merging data</name>
      <command>CADStat.help(doc='loaddata.html')</command>
    </menuitem>
    <menuitem>
      <name>Boxplot</name>
      <command>CADStat.help(doc='bxplot.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Scatterplot</name>
      <command>CADStat.help(doc='scatterplot.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Linear Regression</name>
      <command>CADStat.help(doc='lm.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Regression Prediction</name>
      <command>CADStat.help(doc='glm.pred.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Quantile Regression</name>
      <command>CADStat.help(doc='rq.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Correlation Analysis</name>
      <command>CADStat.help(doc='cor.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Regression Trees</name>
      <command>CADStat.help(doc='rpart.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Conditional Probability</name>
      <command>CADStat.help(doc='conditionalprob.JGR.html')</command>
    </menuitem>
    <menuitem>
      <name>Predicting env. conditions from biological observations</name>
      <command>CADStat.help(doc='bioinfer.JGR.html')</command>
    </menuitem>

  </menu>
  <menu>
    <name>About</name>
    <menuitem>
      <name>About JGR</name>
      <command>.jnew('org/rosuda/JGR/toolkit/AboutDialog')</command>
    </menuitem>
    <menuitem>
      <name>About CADStat</name>
      <command>CADStat.help(doc='CADStat.JGR.html')</command>
    </menuitem>
  </menu>
</menus>
